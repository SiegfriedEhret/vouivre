  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/db": "^0.14.3",
    "@astrojs/vercel": "^7.8.2",
    "astro": "^4.16.7",
    "sharp": "^0.33.5",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1"
  }
}
